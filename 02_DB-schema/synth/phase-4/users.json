{
    "type": "array",
    "length": {
        "type": "number",
        "subtype": "u64",
        "constant": 10
    },
    "content": {
        "type": "object",
        "user_id": {
            "type": "number",
            "subtype": "u64",
            "id": {
                "start_at": 21
            }
        },
        "login": {
            "type": "string",
            "format": {
                "format": "lower(i{name})",
                "arguments": {
                    "name": {
                        "type": "same_as",
                        "ref": "users.content.last_name"
                    }
                }
            }
        },
        "password": {
            "type": "string",
            "pattern": "[$]2[abxy][$]1?[0-9][$][a-zA-Z0-9./]{53}"
        },
        "first_name": {
            "type": "string",
            "faker": {
                "generator": "first_name"
            }
        },
        "last_name": {
            "type": "unique",
            "content": {
                "type": "string",
                "faker": {
                    "generator": "last_name"
                }
            }
        },
        "date_of_birth": {
            "type": "date_time",
            "format": "%d-%m-%Y",
            "subtype": "naive_date",
            "begin": "01-01-1993",
            "end": "31-12-2000"
        }
    }
}
