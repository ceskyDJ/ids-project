{
    "type": "array",
    "length": {
        "type": "number",
        "subtype": "u64",
        "constant": 10
    },
    "content": {
        "type": "object",
        "user_id": {
            "type": "number",
            "subtype": "u64",
            "id": {
                "start_at": 1
            }
        },
        "login": {
            "type": "unique",
            "content": {
                "type": "string",
                "format": {
                    "format": "lower({start}{num})",
                    "arguments": {
                        "start": {
                            "type": "string",
                            "truncated": {
                                "content": {
                                    "type": "string",
                                    "format": {
                                        "format": "x{last}{first}",
                                        "arguments": {
                                            "last": {
                                                "type": "same_as",
                                                "ref": "users.content.last_name"
                                            },
                                            "first": {
                                                "type": "same_as",
                                                "ref": "users.content.first_name"
                                            }
                                        }
                                    }
                                },
                                "length": 6
                            }
                        },
                        "num": {
                            "type": "one_of",
                            "variants": [
                                {
                                    "weight": 0.7,
                                    "type": "string",
                                    "pattern": "0[0-9]"
                                },
                                {
                                    "weight": 0.2,
                                    "type": "string",
                                    "pattern": "[0-9][0-9a-k]"
                                },
                                {
                                    "weight": 0.1,
                                    "type": "string",
                                    "pattern": "[a-z0-9]{2}"
                                }
                            ]
                        }
                    }
                }
            }
        },
        "password": {
            "type": "string",
            "pattern": "[$]2[abxy][$]1?[0-9][$][a-zA-Z0-9./]{53}"
        },
        "first_name": {
            "type": "string",
            "faker": {
                "generator": "first_name"
            }
        },
        "last_name": {
            "type": "string",
            "faker": {
                "generator": "last_name"
            }
        },
        "date_of_birth": {
            "type": "date_time",
            "format": "%d-%m-%Y",
            "subtype": "naive_date",
            "begin": "01-01-1992",
            "end": "31-12-2003"
        }
    }
}
