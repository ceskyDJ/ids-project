{
    "type": "array",
    "length": {
        "type": "number",
        "subtype": "u64",
        "constant": 10
    },
    "content": {
        "type": "object",
        "exam_id": {
            "type": "number",
            "subtype": "u64",
            "id": {}
        },
        "course_id": {
            "type": "same_as",
            "ref": "courses.content.course_id"
        },
        "academic_year": {
            "type": "string",
            "format": {
                "format": "{year}/inc({year})",
                "arguments": {
                    "year": {
                        "type": "number",
                        "range": {
                            "low": 2001,
                            "high": 2022,
                            "step": 1
                        }
                    }
                }
            }
        },
        "'type": {
            "type": "one_of",
            "variants": [
                {
                    "weight": 0.5,
                    "type": "string",
                    "pattern": "midterm"
                },
                {
                    "weight": 0.5,
                    "type": "string",
                    "pattern": "term"
                }
            ]
        },
        "time_limit": {
            "type": "number",
            "range": {
                "low": 10,
                "high": 200,
                "include_low": true,
                "include_high": true
            }
        },
        "max_points": {
            "type": "number",
            "range": {
                "low": 50,
                "high": 100
            }
        },
        "min_points": {
            "type": "number",
            "range": {
                "low": 0,
                "high": 50
            }
        }
    }
}
