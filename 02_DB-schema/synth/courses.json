                            "generator": "industry"
                        }
                    },
                    "part2": {
                        "type": "string",
                        "faker": {
                            "generator": "bs_verb"
                        }
                    },
                    "part3": {
                        "type": "string",
                        "faker": {
                            "generator": "bs_adj"
                        }
                    },
                    "part4": {
                        "type": "string",
                        "faker": {
                            "generator": "bs"
                        }
                    }
                }

            }
